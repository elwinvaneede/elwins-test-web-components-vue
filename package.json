{
  "name": "elwins-test-web-components-vue",
  "sideEffects": false,
  "version": "0.1.0",
  "description": "Vue specific wrapper for my test Web Components library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elwinvaneede/elwins-test-web-components-vue.git"
  },
  "scripts": {
    "test": "../../../node_modules/.bin/jest",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p ."
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.8.0",
    "rollup": "^1.21.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "@vue/test-utils": "^1.0.3",
    "elwins-test-web-components": "^0.1.2",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
